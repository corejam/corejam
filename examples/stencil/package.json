{
  "name": "stencil-shop",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Shop by corejam",
  "scripts": {
    "build": "stencil build",
    "stencil-dev": "stencil build --dev --watch --serve",
    "api": "micro api/graphql.js",
    "dev": "run-p api stencil-dev",
    "static": "run-p api & stencil build --prerender",
    "serve": "run-p api & serve www -l 3001"
  },
  "devDependencies": {
    "@stencil/core": "2.1.0",
    "npm-run-all": "4.1.5",
    "serve": "^11.3.2"
  },
  "license": "MIT",
  "dependencies": {
    "@alepop/stencil-env": "0.4.1",
    "@corejam/plugin-dershop": "0.0.8",
    "@corejam/router": "0.0.5",
    "@rollup/plugin-node-resolve": "9.0.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "micro": "9.3.4",
    "stencil-router-v2": "0.5.1-0"
  }
}
