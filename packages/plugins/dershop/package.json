    "typescript": "4.0.3"
  },
  "dependencies": {
    "@alepop/stencil-env": "0.4.1",
    "@corejam/base": "0.0.8",
    "@corejam/core-components": "0.0.8",
    "@corejam/plugin-auth": "0.0.8",
    "@corejam/rollup-plugin": "0.0.1",
    "@corejam/router": "0.0.5",
