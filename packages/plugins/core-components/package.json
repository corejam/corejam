    "@stencil/core": "2.3.0",
    "@stencil/react-output-target": "0.0.9",
    "@stencil/store": "1.3.0",
    "@types/faker": "5.1.5",
    "@xstate/fsm": "1.5.2",
    "dotenv": "8.2.0",
    "shx": "0.3.3",
    "stencil-router-v2": "0.6.0",
    "tslib": "2.0.3",
    "typescript": "4.1.3",
    "xstate": "4.15.1"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "corejam": {
    "wrapper": [
      "corejam-init"
    ],
    "recommendations": [
      "corejam-error",
      "corejam-ui-base"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.3.6",
