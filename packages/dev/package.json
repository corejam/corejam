{
  "name": "@corejam/dev",
  "version": "0.0.5",
  "description": "Corejam Dev Components",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dev/dev.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "stencil build --dev --watch --serve",
    "build": "stencil build --docs && yarn build:plugin",
    "build:plugin": "rollup --config rollup.config.js",
    "postbuild": "shx cp ./app/index.html ./dist",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@corejam/router": "0.0.5",
    "@stencil/core": "2.0.3",
    "stencil-router-v2": "0.5.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-typescript": "6.0.0",
    "fs-jetpack": "3.1.0",
    "rollup": "2.30.0",
    "shx": "0.3.2"
  }
}
