{
  "name": "@corejam/derblog",
  "version": "0.0.1",
  "description": "Statically generated Blog built with Corejam",
  "private": true,
  "license": "MIT",
  "main": "web-components/index.cjs.js",
  "module": "web-components/custom-elements/index.js",
  "es2015": "web-components/esm/index.js",
  "es2017": "web-components/esm/index.js",
  "types": "web-components/custom-elements/index.d.ts",
  "collection": "web-components/collection/collection-manifest.json",
  "collection:main": "web-components/collection/index.js",
  "scripts": {
    "dev": "corejam dev",
    "build": "corejam build",
    "test": "jest --verbose --config=./jest.config.js",
    "precommit": "lint-staged",
    "static": "corejam static -l",
    "static:serve": "corejam static:serve",
    "clean": "rimraf dist & rimraf react & rimraf web-components"
  },
  "files": [
    "dist",
    "web-components"
  ],
  "devDependencies": {
    "@corejam/base": "0.0.11",
    "@corejam/cli": "0.0.19",
    "@corejam/core-components": "0.0.20",
    "@corejam/dev": "0.0.9",
    "@corejam/rollup-plugin": "0.0.3",
    "@corejam/run": "0.0.4",
    "@rollup/plugin-replace": "2.3.4",
    "@stencil/core": "2.3.0",
    "@stencil/react-output-target": "0.0.9",
    "@types/faker": "5.1.5",
    "dotenv": "8.2.0",
    "faker": "5.1.0",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "corejam": {
    "wrapper": [],
    "recommendations": []
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@stencil/store": "1.3.0",
    "graphql-tag": "2.11.0"
  }
}
