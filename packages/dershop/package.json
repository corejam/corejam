    "jest-date-mock": "1.0.8",
    "shx": "0.3.3",
    "tslib": "2.2.0",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@alepop/stencil-env": "0.4.1",
    "@corejam/base": "0.0.14",
    "@corejam/core-components": "0.0.29",
    "@corejam/notify": "0.0.8",
    "@corejam/plugin-auth": "0.0.29",
    "@corejam/plugin-canvas": "0.0.11",
    "@corejam/rollup-plugin": "0.0.8",
    "@corejam/run": "0.0.13",
    "@stencil/core": "2.4.0",
    "@stencil/store": "1.4.1",
    "@xstate/fsm": "1.6.0",
