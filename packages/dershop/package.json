    "stencil-router-v2": "0.6.0",
    "xstate": "4.16.2"
  },
  "lint-staged": {
    "server/**/*.ts": [
      "prettier --write"
    ],
    "shared/**/*.ts": [
      "prettier --write"
    ],
    "app/**/!(*.d).ts": [
      "prettier --write"
    ],
    "app/**/*.tsx": [
      "prettier --write"
    ]
  },
  "corejam": {
    "wrapper": [],
    "recommendations": []
